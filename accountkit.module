<?php

/**
 * @file
 * Contains accountkit.module.
 */

/**
 * Implements hook_theme().
 */
function accountkit_theme() {
  $theme['accountkit_login_form'] = [
    'template' => 'accountkit_login_form',
    'render element' => 'html'
  ];

  $theme['accountkit_success'] = [
    'template' => 'accountkit_success',
    'render element' => 'html'
  ];

  return $theme;
}